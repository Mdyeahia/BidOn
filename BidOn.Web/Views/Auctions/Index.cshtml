@model List<BidOn.Entities.Auction>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table>
    <thead>
        <tr>
            <td>Title</td>
            <br />
            <td>Description</td>
            <br />
            <td>ActualAmount</td>
            <br />
            <td>StartingTime</td>
            <br />
            <td>EndTime</td>
        </tr>
       
    </thead>
    <tbody>
        
            @foreach (var auction in Model)
            {
                <tr>
                    <td>@auction.Title</td>
                    
                    <td>@auction.Description</td>
                    
                    <td>@auction.ActualAmount</td>
                    
                    <td>@auction.StartingTime</td>
                    
                    <td>@auction.EndTime</td>
                    
                    
                </tr>
            }
    </tbody>
</table>